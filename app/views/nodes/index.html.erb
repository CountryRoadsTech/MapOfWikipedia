<p id="notice"><%= notice %></p>

<h1>Nodes</h1>

<table>
  <thead>
    <tr>
      <th>Graph</th>
      <th>Name</th>
      <th>Url</th>
      <th>Summary</th>
      <th>Content</th>
      <th>Marked up content</th>
      <th>Began processing links</th>
      <th>Ended processing links</th>
      <th>Error processing links</th>
      <th>Error message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nodes.each do |node| %>
      <tr>
        <td><%= node.graph_id %></td>
        <td><%= node.name %></td>
        <td><%= node.url %></td>
        <td><%= node.summary %></td>
        <td><%= node.content %></td>
        <td><%= node.marked_up_content %></td>
        <td><%= node.began_processing_links %></td>
        <td><%= node.ended_processing_links %></td>
        <td><%= node.error_processing_links %></td>
        <td><%= node.error_message %></td>
        <td><%= link_to 'Show', node %></td>
        <td><%= link_to 'Edit', edit_node_path(node) %></td>
        <td><%= link_to 'Destroy', node, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Node', new_node_path %>
